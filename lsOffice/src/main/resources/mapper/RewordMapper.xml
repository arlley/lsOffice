<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.arlley.pack.mapper.RewordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.arlley.pack.entity.Reword">
        <id column="id" property="id" />
        <result column="reward_time" property="time" />
        <result column="reward_rank" property="rank" />
        <result column="authority" property="authority" />
        <result column="content" property="content" />
        <result column="lid" property="lid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, reward_time, reward_rank, authority, content, lid
    </sql>

</mapper>
